{
  "name": "musda-himperra-lampung",
  "version": "1.0.0",
  "description": "Sistem Manajemen Event MUSDA II HIMPERRA Lampung",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "npm run build:frontend",
    "build:frontend": "cd frontend && npm run build",
    "start": "cd backend && npm start",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "deploy": "deploy.cmd",
    "deploy:unix": "chmod +x deploy.sh && ./deploy.sh",
    "update:api": "node update-api-urls.js"
  },
  "dependencies": {
    "@types/html2canvas": "^0.5.35",
    "@types/react": "^19.1.12",
    "@zxing/browser": "^0.1.5",
    "bcrypt": "^6.0.0",
    "canvas": "^3.2.0",
    "html2canvas": "^1.4.1",
    "html5-qrcode": "^2.3.8",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "mysql2": "^3.15.1",
    "node-fetch": "^3.3.2",
    "react-qr-reader": "^3.0.0-beta-1"
  },
  "devDependencies": {
    "@types/react-qr-reader": "^2.1.7",
    "concurrently": "^7.6.0"
  }
}
