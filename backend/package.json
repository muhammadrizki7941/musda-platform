{
  "name": "musda-backend",
  "version": "1.0.0",
  "description": "Backend Buku Tamu Interaktif MUSDA II HIMPERRA Lampung",
  "main": "index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node src/index.js",
    "build": "echo 'Backend build complete'",
    "setup": "node src/utils/setupDatabase.js",
    "seed": "node src/seeders/DatabaseSeeder.js",
    "seed:fresh": "node src/seeders/DatabaseSeeder.js --fresh",
    "seed:users": "node src/seeders/UserSeeder.js",
    "migrate": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "migrate:fresh": "knex migrate:rollback --all && knex migrate:latest",
    "db:seed": "knex seed:run",
    "db:fresh": "npm run migrate:fresh && npm run db:seed",
    "sql:migrate": "node scripts/run-sql-migrations.js --apply",
    "sql:migrate:dry": "node scripts/run-sql-migrations.js",
    "env:print": "node scripts/print-env.js",
    "schema:bundle": "node scripts/build-schema.js"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "body-parser": "^1.20.2",
    "busboy": "^1.6.0",
    "canvas": "^3.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "multer": "^2.0.2",
    "mysql2": "^3.9.4",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.6",
    "pdfkit": "^0.17.2",
    "qrcode": "^1.5.4",
    "react-slick": "^0.31.0",
    "resend": "^6.1.2",
    "sanitize-html": "^2.17.0",
    "sharp": "^0.33.3",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "sequelize-cli": "^6.6.3"
  }
}
